{
	"name": "catchmentnet",
	"version": "1.0.0",
	"description": "Obsidian plugin to sync Substack newsletters from Gmail",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"gmail",
		"substack",
		"newsletter"
	],
	"author": "Robby Rao",
	"license": "MIT",
	"dependencies": {
		"nostr-tools": "^2.17.0"
	},
	"optionalDependencies": {
		"server-destroy": "^1.0.1"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"jsdom": "^26.1.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.9.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/robrao/CatchmentNet.git"
	},
	"bugs": {
		"url": "https://github.com/robrao/CatchmentNet/issues"
	},
	"homepage": "https://github.com/robrao/CatchmentNet#readme"
